package edu.neumont.csc252;

public class FrequencyNode<T> implements Comparable<T>{

	private FrequencyNode left = null;
	private FrequencyNode right = null;
	
	private double frequency = 0;
	private int value = 0;
	
	public FrequencyNode(int frequency, int value){
		
		this.frequency = frequency;
		this.value = value;
	}

	public FrequencyNode getLeft() {
		return left;
	}

	public void setLeft(FrequencyNode left) {
		this.left = left;
	}

	public FrequencyNode getRight() {
		return right;
	}

	public void setRight(FrequencyNode right) {
		this.right = right;
	}

	public double getFrequency() {
		return frequency;
	}

	public int getValue() {
		return value;
	}

	@Override
	public int compareTo(T arg0) {
		// TODO Auto-generated method stub
		return 0;
	}

}
