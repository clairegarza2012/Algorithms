package edu.neumont.csc252;

public class Node {

	private int parent;
	private int weight;
	private int energy;
	
	public Node(int energy){
		this.energy = energy;
	}

	public int getParent() {
		return parent;
	}

	public void setParent(int parent) {
		this.parent = parent;
	}

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}
	
	public int getEnergy(){
		return this.energy;
	}
	
}
