package edu.neumont.csc252.test;

import java.util.Iterator;

import org.junit.Test;

import edu.neumont.csc252.Heap;

public class AvlPriorityQueueTest {

	@Test
	public void test() {
		
		Heap<Integer> heap = new Heap<Integer>();
		heap.insert(3);
		heap.insert(4);
		heap.insert(2);		
		heap.insert(5);
		heap.insert(7);
		heap.insert(1);	
		
		Iterator<Integer> it = heap.getIterator();
		while (it.hasNext()){
			System.out.print(it.next() + ", ");
		}
		System.out.println();
		
		System.out.println(heap.getSmallest());
		System.out.println(heap.removeSmallest());
		System.out.println();
		Iterator<Integer> it2 = heap.getIterator();
		while (it2.hasNext()){
			System.out.print(it2.next() + ", ");
		}
		System.out.println();
		
		System.out.println(heap.getSmallest());
		System.out.println(heap.removeSmallest());
		System.out.println();
		Iterator<Integer> it3 = heap.getIterator();
		while (it3.hasNext()){
			System.out.print(it3.next() + ", ");
		}
		System.out.println();
		
		System.out.println(heap.getSmallest());
		System.out.println(heap.removeSmallest());
		System.out.println();
		Iterator<Integer> it4 = heap.getIterator();
		while (it4.hasNext()){
			System.out.print(it4.next() + ", ");
		}
		System.out.println();
		
		System.out.println(heap.getSmallest());
		System.out.println(heap.removeSmallest());
		System.out.println();
		Iterator<Integer> it5 = heap.getIterator();
		while (it5.hasNext()){
			System.out.print(it5.next() + ", ");
		}
		System.out.println();
		
		System.out.println(heap.getSmallest());
		System.out.println(heap.removeSmallest());
		System.out.println();
		Iterator<Integer> it6 = heap.getIterator();
		while (it6.hasNext()){
			System.out.print(it6.next() + ", ");
		}
		
	}

}
