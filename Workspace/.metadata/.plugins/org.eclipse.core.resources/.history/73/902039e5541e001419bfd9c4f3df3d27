package edu.neumont.csc250;

public class InsertBinaryTree {

	public static void main(String[] args){
		
	}
	
	public static boolean insert(Object data){
		
		TreeNode root = new TreeNode();
		
		if (root == null)
			return false;
		
		insert(root, data);
		
		return true;
	}
	
	private static void insert(TreeNode root, Object k){
		
		if (root == null){
			return;
		}
		
		if (root)
		
		if (root.compareTo(k) > 0){
			insert(root.leftDecsendent, k);
		}
		else if (root.compareTo(k) < 0){
			insert(root.rightDecsendent, k);
		}
		else{
			return;
		}
		
	}
}
