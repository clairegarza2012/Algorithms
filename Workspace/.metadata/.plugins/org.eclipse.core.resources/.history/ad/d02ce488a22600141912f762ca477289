package edu.neumont.csc252.test;

import java.util.Iterator;

import org.junit.Test;

import edu.neumont.csc252.Heap;

public class AvlPriorityQueueTest {

	@Test
	public void test() {
		
		Heap<Integer> heap = new Heap<Integer>();
		heap.insert(3);
		System.out.println("Finished Inserting 3");
		heap.insert(4);
		System.out.println("Finished Inserting 4");
		heap.insert(2);		
		System.out.println("Finished Inserting 2");
		
		Iterator<Integer> it = heap.getIterator();
		
		while (it.hasNext()){
			System.out.println(it.next());
		}
	}

}
