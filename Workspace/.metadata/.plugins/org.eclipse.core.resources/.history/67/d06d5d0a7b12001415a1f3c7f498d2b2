		Client client1 = new Client(5);
		Client client2 = new Client(6);
		Client client3 = new Client(3);
		Client client4 = new Client(10);
		Client client5 = new Client(1);
		Client client11 = new Client(5);
		Client client12 = new Client(6);
		Client client13 = new Client(3);
		Client client14 = new Client(10);
		Client client15 = new Client(1);

		store.addClient(client1);
		store.addClient(client2);
		store.addClient(client3);
		store.addClient(client4);
		store.addClient(client5);
		store.addClient(client11);
		store.addClient(client12);
		store.addClient(client13);
		store.addClient(client14);
		store.addClient(client15);

		System.out.println(store.getClientWaitTime(client4));
		store.printLines();
		
	}
	
}
