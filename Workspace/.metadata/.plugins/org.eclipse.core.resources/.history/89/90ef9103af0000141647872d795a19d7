package edu.neumont.csc250;

public class GCD {

	public static void main(String[] args) {
	
		int m = 36;
		int n = 3;
		int result = findGCD(m, n);
		
		System.out.println(result);
	}

	private static int findGCD(int m, int n) {
		
		if (n > m){
			int temp = m;
			n = m;
			m = temp;
		}
		
		if(n == 0)
		{
			return m;
		}
		else
		{
			findGCD(n, m % n);
		}
		
		return m;
	}

}
